{
  "name": "flex-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev",
    "postinstall": "node ./scripts/fix-missing-esm-entries.mjs",
    "build": "tsc -p convex -noEmit && tsc -p . -noEmit && vite build",
    "build:frontend": "vite build",
    "build:backend": "convex deploy",
    "preview": "vite preview",
    "lint": "tsc -p convex -noEmit --pretty false && tsc -p . -noEmit --pretty false && convex dev --once && vite build",
    "clear": "convex run clear:clearData"
  },
  "dependencies": {
    "@auth/core": "^0.37.4",
    "@capacitor/android": "^8.1.0",
    "@capacitor/cli": "^8.1.0",
    "@capacitor/core": "^8.1.0",
    "@capacitor/local-notifications": "^8.0.1",
    "@convex-dev/auth": "^0.0.90",
    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "framer-motion": "^12.34.2",
    "isexe": "^3.1.4",
    "jszip": "^3.10.1",
    "lucide-react": "^0.556.0",
    "pdf-parse": "^2.4.5",
    "pdfium-wasm": "^0.0.2",
    "pdfjs-dist": "^5.4.624",
    "pngjs": "^7.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "react-pdf": "^10.3.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jszip": "^3.4.1",
    "@types/node": "^22.19.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "~10",
    "dotenv": "^16.6.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "~8",
    "prettier": "^3.8.1",
    "tailwindcss": "~3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.55.0",
    "vite": "^6.2.0"
  },
  "overrides": {
    "string-width": "4.2.3"
  }
}
